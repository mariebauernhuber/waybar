{
    "layer": "top",
    "position": "bottom",
    "height": 32,
    "modules-left": [
        "custom/archlogo",
        "hyprland/workspaces",
        "hyprland/window"
    ],
    "modules-center": [
        "mpris#browser",
        "mpris#strwb",
        "mpris#spotify" 
    ],
    "modules-right": [
        "tray",
        "pulseaudio",
        "hyprland/language",
        "network",
        "custom/vpn",
	"temperature",
	"battery",
        "memory",
        "cpu",
        "clock",
	"custom/lesson-warning"
    ],
    "custom/archlogo": {
        "exec": "echo 'ÔåÉ'",
        "interval": 0,
        "format": "{}",
        "tooltip-format": "Arch Linux BTW",
        "on-click": "alacritty -e bash -c 'fastfetch; $SHELL'"
    },
    "hyprland/workspaces": {
        "on-scroll-up": "hyprctl dispatch workspace e-1",
        "on-scroll-down": "hyprctl dispatch workspace e+1"
    },
    "mpris#browser": {
        "player": "firefox",
        "format": " üè¥‚Äç‚ò†Ô∏è {status_icon} {title} ",
        "status-icons": {
            "playing": "ÔÅã",
            "paused": "ÔÅå",
            "stopped": "ÔÅç"
        },
        "max-length": 20,
        "on-click": "playerctl --player=firefox play-pause",
        "on-click-right": "playerctl --player=firefox stop"
    },
    "mpris#strwb": {
        "player": "strawberry",
        "format": " üçì {status_icon} {artist} ‚Äî {title} ",
        "status-icons": {
            "playing": "ÔÅã",
            "paused": "ÔÅå",
            "stopped": "ÔÅç"
        },
        "max-length": 50,
        "on-click": "playerctl --player=strawberry play-pause",
        "on-click-right": "playerctl --player=strawberry stop",
        "on-scroll-up": "playerctl --player=strawberry previous",
        "on-scroll-down": "playerctl --player=strawberry next"
    },
    "mpris#spotify": {
        "player": "spotify",
        "format": " ÔÜº {artist} ‚Äî {title} {status_icon} ",
        "status-icons": {
            "playing": "ÔÅã",
            "paused": "ÔÅå",
            "stopped": "ÔÅç"
        },
        "max-length": 50,
        "on-click": "playerctl --player=spotify play-pause",
        "on-click-right": "playerctl --player=spotify stop",
        "on-scroll-up": "playerctl --player=spotify previous",
        "on-scroll-down": "playerctl --player=spotify next"
    },
    "tray": {
        "icon-size": 17,
        "spacing": 10
    },
    "pulseaudio": {
        "format": "{icon}{volume}% ",
        "format-muted": "<span color='#FF4040'> Û∞ñÅ </span>Muted ",
        "format-icons": {
            "headset": " ÔÄ•  ",
            "default": [
                "<span color='#808080'> ÔÄ¶ </span>",
                "<span color='#FFFF66'> ÔÄß </span>",
                "<span color='#00FF7F'> ÔÄ® </span>"
            ]
        },
        "on-click-right": "pactl -- set-sink-mute 0 toggle",
        "on-click": "pavucontrol -t 3"
    },
    "hyprland/language": {
        "format-en": " :3 ",
        "format-ru": " ru "
    },
    "network": {
        "format-wifi": "<span color='#00FFFF'> Û∞§® </span> {essid} ",
        "format-ethernet": "<span color='#7FFF00'> ÔÄí </span> Wired ",
        "tooltip-format": "<span color='#00BFFF'> Û∞Ö¢ </span> {bandwidthDownBytes} <span color='#FF1493'> Û∞Öß </span> {bandwidthUpBytes} ",
        "format-linked": "<span color='#FFA500'> Û±òñ </span> {ifname} (No IP) ",
        "format-disconnected": "<span color='#FF4040'> ÔÅ± </span> Disconnected ",
        "format-alt": "<span color='#00FFFF'> Û∞§® </span> {signalStrength}% ",
        "interval": 2
    },
	"custom/vpn": { 
        "exec": "$HOME/.config/waybar/scripts/vpn_status.sh", 
        "interval": 5, 
        "return-type": "json",
        "on-click": "sudo vpn toggle", 
        "tooltip": false 
    },
	"temperature": {
	"hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
	"format": "{temperatureC}¬∞C {icon}",
	"format-icons": ["Ôãâ", "Ôãà", "Ôãá"],
	"critical-threshold": 80,
	"format-critical": "{temperatureC}¬∞C ÔÅ±"
	},
    "battery": {
	"states": {
		"warning": 30,
		"critical": 15
	},
	"format": " ÔâÄ  {capacity}%",
	"format-warning": " ÔÅ±  {capacity}%",
	"format-critical": " ÔÅ±  {capacity}%",
	"interval": 3
	},
    "memory": {
        "format": " ÓøÖ  {used:0.1f}GB ",
        "tooltip": true,
        "tooltip-format": " {used:0.2f}GB / {total:0.2f}GB ",
        "on-click": "alacritty -e btop",
        "interval": 3
    },
    "cpu": {
        "format": " Óâ¶  {usage}% ",
        "tooltip": true,
        "on-click": "alacritty -e btop",
        "interval": 3
    },
    "clock": {
	"timezone": "Europe/Berlin",
	"tooltip-format": "{:%Y-%m-%d %A}",
	"format-alt": "{:%Y-%m-%d}",
	"format": "Û∞•î {:%H:%M:%S}",
	"interval": 1
	},
"custom/lesson-warning": {
    "exec": "~/.config/waybar/scripts/lesson-warning.sh",
    "interval": 30,
    "format": "{}",
    "return-type": "json"
}

}
